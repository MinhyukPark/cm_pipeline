[default]
network_name = cit_patents 
output_dir = ~/sample_data
algorithm = leiden
resolution = 0.5,0.01,0.0001
number_of_iterations = 2
cm_version = new
existing_op_json = ./existing_op_files.json

[connectivity_modifier]
threshold = 1log10
# below config keys are used only for new version of CM
nprocs = 32
labelonly = 1
quiet = 1

