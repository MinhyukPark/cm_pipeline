# Currently each stage runs one after the other but the input files are not yet taken from the output of the 
# previous stage. This link will be created after all the stages are tested.

[cleanup]
script=./scripts/cleanup_el.R
inputfile = ~/sample_data/cit_patents_cleaned.tsv
outputdir = ~/sample_data/output

[clustering]  #stage name
algorithm = leiden
resolution = 0.5 
inputfile = ~/sample_data/cit_patents_cleaned.tsv
outputdir = ~/sample_data/output

[filtering]
script=./scripts/subset_graph_nonetworkit_treestar.R
inputfile = ~/sample_data/cit_patents_cleaned.tsv
clusteringfile = ~/cit_patents_leiden.1_rep1/cit_patents_leiden.1_rep1.tsv
outputfiletag = cit_patents_trimmed